{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "preserve",
        "baseUrl": ".",
        "paths": {
            "~/*": ["src/*"]
        },
        "noUnusedLocals": true,
        "noUncheckedIndexedAccess": true,
        "noUnusedParameters": true,
        "strictNullChecks": true
    }
}
